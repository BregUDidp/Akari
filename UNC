local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "   |   Akari Hub   |    UNC Test Executer    |" ,
   Icon = "app-window",
   LoadingTitle = "   Akari Hub  UNC   ",
   LoadingSubtitle = "",
   ShowText = " üåë ",
   Theme = "Default",
   ToggleUIKeybind = "G",
   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Akari"
   },
   KeySystem = false
})

-- Tab UNC (‡πÄ‡∏î‡∏¥‡∏°)
local TabUNC = Window:CreateTab("||   - Unc Test Executer", "app-window-mac")
TabUNC:CreateSection("")

local UNCResultLabel = TabUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local UNCStatusLabel = TabUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local UNCDetectLabel = TabUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local isTestingUNC = false
local dots = {"", ".", "..", "..."}

local function performUNCTest()
   local success, total = 0, 0
   
   local uncFunctions = {
      getrawmetatable, setreadonly, make_readonly, isreadonly,
      getrenv, getgc, getreg, gethui,
      hookfunction, hookmetamethod, newcclosure,
      firesignal, getconnections, getconnectionstate,
      cloneref, cache.invalidate, cache.iscached,
      decompile, getupvalue, setupvalue, getupvalues,
      getconstants, getconstant, getprotos, getproto,
      is_synapse_function, isexecutorclosure, checkcaller,
   }
   
   for _, func in ipairs(uncFunctions) do
      total = total + 1
      local s = pcall(function() return func ~= nil and (type(func) == "function" or type(func) == "table") end)
      if s then success = success + 1 end
   end
   
   return math.floor((success / total) * 100 + 0.5)
end

local function showUNCResult(percent)
   local colorCode = percent >= 95 and "rgb(0, 255, 100)" or
                     percent >= 85 and "rgb(50, 255, 150)" or
                     percent >= 70 and "rgb(255, 220, 0)" or
                     "rgb(255, 70, 70)"
   
   UNCResultLabel:Set('<font color="' .. colorCode .. '"><b>Your Unc Test Executer:  ' .. percent .. ' %</b></font>')
   
   local statusText = percent >= 95 and "Unc Test Executer Rate:  100 %" or
                      percent >= 85 and "Unc Test Executer Rate:  85 %" or
                      percent >= 70 and "Unc Test Executer Rate:  70 %" or
                      "Your Executer Unc Test:  0 %"
   local statusColor = percent >= 95 and "rgb(0,255,100)" or percent >= 85 and "rgb(255,215,0)" or percent >= 70 and "rgb(255,215,0)" or "rgb(255,80,80)"
   
   UNCStatusLabel:Set('<font color="'..statusColor..'"><b>' .. statusText .. '</b></font>')
   
   UNCDetectLabel:Set(percent >= 92 and '<font color="rgb(0,255,120)"><b>Undetected Executer  100 %</b></font>' or '<font color="rgb(255,80,80)"><b>Detected Executer  100 %</b></font>')
   
   task.delay(7, function()
      UNCResultLabel:Set('<font color="rgb(0, 255, 100)"><b> --- </b></font>')
      UNCStatusLabel:Set('<font color="rgb(0, 255, 120)"><b> --- </b></font>')
      UNCDetectLabel:Set('<font color="rgb(0, 255, 100)"><b> --- </b></font>')
   end)
end

TabUNC:CreateButton({
   Name = "                                                                      ‚ñ∂Ô∏è",
   Callback = function()
      if isTestingUNC then return end
      isTestingUNC = true
      
      local dotIndex = 1
      local conn = game:GetService("RunService").Heartbeat:Connect(function()
         if not isTestingUNC then conn:Disconnect() return end
         UNCStatusLabel:Set("Checking Unc" .. dots[dotIndex])
         dotIndex = (dotIndex % #dots) + 1
      end)
      
      task.spawn(function()
         local percent = performUNCTest()
         isTestingUNC = false
         conn:Disconnect()
         showUNCResult(percent)
      end)
   end,
})

TabUNC:CreateSection("")

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡∏î‡πâ‡∏ß‡∏¢‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏ï‡∏±‡πâ‡∏á‡πÅ‡∏ï‡πà‡πÅ‡∏£‡∏Å
local RejoinCountdownLabel = TabUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

TabUNC:CreateButton({
   Name = "                                                                      üï∞Ô∏è",
   Callback = function()
      local countdown = 5
      
      spawn(function()
         while countdown > 0 do
            -- ‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á countdown ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
            RejoinCountdownLabel:Set(
               '<font color="rgb(255, 215, 0)"><b>Wait Rejoin  (  ' .. countdown .. '  )</b></font>'
            )
            task.wait(1)
            countdown = countdown - 1
         end
        
         -- ‡∏ï‡∏≠‡∏ô‡∏£‡∏µ‡∏à‡∏≠‡∏¢‡∏à‡∏£‡∏¥‡∏á ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏™‡∏î
         RejoinCountdownLabel:Set(
            '<font color="rgb(0, 255, 120)"><b>Rejoining Now</b></font>'
         )
         task.wait(0.7)
         
         -- ‡∏£‡∏µ‡∏à‡∏≠‡∏¢
         local success, err = pcall(function()
            local ts = game:GetService("TeleportService")
            local p = game:GetService("Players").LocalPlayer
            ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, p)
         end)
         
         if not success then
            -- ‡∏ñ‡πâ‡∏≤‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÅ‡∏î‡∏á
            RejoinCountdownLabel:Set(
               '<font color="rgb(255, 80, 80)"><b>Rejoin Failed: ' .. tostring(err) .. '</b></font>'
            )
            task.delay(5, function()
               -- ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏î‡∏¥‡∏°‡∏´‡∏•‡∏±‡∏á 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
               RejoinCountdownLabel:Set(
                  '<font color="rgb(0, 255, 100)"><b>üï∞Ô∏è Ready to Rejoin</b></font>'
               )
            end)
         end
      end)
   end,
})

-- Tab sUNC (‡πÉ‡∏´‡∏°‡πà!)
local TabSUNC = Window:CreateTab("||   - Sunc Test Executer", "app-window-mac")
TabSUNC:CreateSection("")

local sUNCResultLabel = TabSUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local sUNCStatusLabel = TabSUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local sUNCDetectLabel = TabSUNC:CreateLabel(
   '<font color="rgb(0, 255, 100)"><b> --- </b></font>'
)

local isTestingSUNC = false

local function performsUNCTest()
   local success, total = 0, 0
   
   -- ‡∏ï‡∏±‡∏ß‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡πá‡∏Ñ sUNC ‡πÅ‡∏ö‡∏ö‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏à‡∏£‡∏¥‡∏á (‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏ï‡∏≤‡∏° trend 2026)
   local suncTests = {
      function() -- hookfunction ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏à‡∏£‡∏¥‡∏á
         local original = warn
         local hooked = hookfunction(warn, function(...) return original("hooked!") end)
         return hooked == original
      end,
      function() -- newcclosure ‡∏™‡∏£‡πâ‡∏≤‡∏á closure ‡πÉ‡∏´‡∏°‡πà‡∏à‡∏£‡∏¥‡∏á
         local f = newcclosure(function() return 1 end)
         return type(f) == "function" and iscclosure(f) == false
      end,
      function() -- getrawmetatable ‡πÉ‡∏ä‡πâ‡πÑ‡∏î‡πâ‡∏à‡∏£‡∏¥‡∏á
         local mt = getrawmetatable(game)
         return type(mt) == "table" and rawget(mt, "__index") ~= nil
      end,
      function() return firesignal ~= nil end, -- ‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô‡πÅ‡∏ï‡πà‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏û‡∏¥‡πà‡∏°
      function() return getconnections ~= nil end,
      -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÑ‡∏î‡πâ‡∏≠‡∏µ‡∏Å‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (sUNC ‡∏à‡∏£‡∏¥‡∏á‡∏°‡∏µ 50+ tests ‡πÅ‡∏ï‡πà‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡∏ô‡∏á‡πà‡∏≤‡∏¢)
   }
   
   for _, test in ipairs(suncTests) do
      total = total + 1
      local ok = pcall(test)
      if ok then success = success + 1 end
   end
   
   return math.floor((success / total) * 100 + 0.5)
end

local function showSUNCResult(percent)
   local colorCode = percent >= 95 and "rgb(0, 255, 100)" or
                     percent >= 85 and "rgb(50, 255, 150)" or
                     percent >= 70 and "rgb(255, 220, 0)" or
                     "rgb(255, 70, 70)"
   
   sUNCResultLabel:Set('<font color="' .. colorCode .. '"><b>Your Sunc Test Executer:  ' .. percent .. ' %</b></font>')
   
   local statusText = percent >= 95 and "Sunc Test Executer Rate:  100 %" or
                      percent >= 85 and "Sunc Test Executer Rate:  85 %" or
                      percent >= 70 and "Sunc Test Executer Rate:  70 %" or
                      "Your Executer Sunc Test:  0 %"
   local statusColor = percent >= 95 and "rgb(0,255,100)" or percent >= 85 and "rgb(0,220,255)" or percent >= 70 and "rgb(255,215,0)" or "rgb(255,80,80)"
   
   sUNCStatusLabel:Set('<font color="'..statusColor..'"><b>' .. statusText .. '</b></font>')
   
   sUNCDetectLabel:Set(percent >= 95 and '<font color="rgb(0,255,120)"><b>Undetected Executer  100 %</b></font>' or '<font color="rgb(255,80,80)"><b>Detected Executer  100 %</b></font>')
   
   task.delay(7, function()
      sUNCResultLabel:Set('<font color="rgb(0, 255, 100)"><b> --- </b></font>')
      sUNCStatusLabel:Set('<font color="rgb(0, 255, 120)"><b> --- </b></font>')
      sUNCDetectLabel:Set('<font color="rgb(0, 255, 100)"><b> --- </b></font>')
   end)
end

TabSUNC:CreateButton({
   Name = "                                                                      ‚ñ∂Ô∏è",
   Callback = function()
      if isTestingSUNC then return end
      isTestingSUNC = true
      
      local dotIndex = 1
      local conn = game:GetService("RunService").Heartbeat:Connect(function()
         if not isTestingSUNC then conn:Disconnect() return end
         sUNCStatusLabel:Set("Checking sUNC" .. dots[dotIndex])
         dotIndex = (dotIndex % #dots) + 1
      end)
      
      task.spawn(function()
         local percent = performsUNCTest()
         isTestingSUNC = false
         conn:Disconnect()
         showSUNCResult(percent)
      end)
   end,
})

