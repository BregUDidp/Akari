local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Window - ‡∏ä‡∏∑‡πà‡∏≠ Roblox ‡∏Ñ‡∏∏‡∏ì‡πÄ‡∏î‡πà‡∏ô‡∏™‡∏∏‡∏î!
local Window = Rayfield:CreateWindow({
   Name = "   |   Akari Hub   |   UNC Test Executer    |" ,
   Icon = "app-window",
   LoadingTitle = "   Akari Hub  UNC   ",

   LoadingSubtitle = "",
   
   ShowText = " üåë ",
   Theme = "Default",
   ToggleUIKeybind = "G",
   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "akari"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "",
      Subtitle = "",
      Note = "",
      FileName = "",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"üåë"}
   }
})

local Tab = Window:CreateTab("||   - Unc Test Executer", "app-window-mac")

Tab:CreateSection("")

local Label = Tab:CreateLabel(" Launching Executer", "play")

Tab:CreateSection("")

-- Label ‡∏´‡∏•‡∏±‡∏Å (‡∏ú‡∏• %)
local ResultLabel = Tab:CreateLabel("  ---  ")

-- Label ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ + Loading animation
local StatusLabel = Tab:CreateLabel("  ---  ")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏° loading animation
local isTesting = false
local dots = {"", ".", "..", "..."}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏î‡∏™‡∏≠‡∏ö UNC (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏° ‡πÅ‡∏Ñ‡πà‡πÅ‡∏¢‡∏Å‡∏™‡πà‡∏ß‡∏ô)
local function performUNCTest()
   local success, total = 0, 0
   
   local uncFunctions = {
      getrawmetatable, setreadonly, make_readonly, isreadonly,
      getrenv, getgc, getreg, gethui,
      hookfunction, hookmetamethod, newcclosure,
      firesignal, getconnections, getconnectionstate,
      cloneref, cache.invalidate, cache.iscached,
      decompile, getupvalue, setupvalue, getupvalues,
      getconstants, getconstant, getprotos, getproto,
      is_synapse_function, isexecutorclosure, checkcaller,
   }
   
   for _, func in ipairs(uncFunctions) do
      total = total + 1
      local s = pcall(function()
         return func ~= nil and (type(func) == "function" or type(func) == "table")
      end)
      if s then
         success = success + 1
      end
   end
   
   return math.floor((success / total) * 100 + 0.5)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏• % ‡πÅ‡∏•‡∏∞‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local function showResult(percent)
   -- ‡πÅ‡∏ó‡∏ô‡∏ó‡∏µ‡πà‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡πÄ‡∏î‡∏¥‡∏°‡∏ô‡∏µ‡πâ:
-- ResultLabel:Set("Unc Test Script:  " .. percent .. " %")

-- ‡∏î‡πâ‡∏ß‡∏¢‡πÇ‡∏Ñ‡πâ‡∏î‡∏ô‡∏µ‡πâ‡πÅ‡∏ó‡∏ô‡∏Ñ‡∏£‡∏±‡∏ö
local colorCode
if percent >= 95 then
    colorCode = "rgb(0, 255, 100)"     -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏™‡∏î God Tier
elseif percent >= 85 then
    colorCode = "rgb(50, 255, 150)"    -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏≠‡πà‡∏≠‡∏ô Perfect
elseif percent >= 70 then
    colorCode = "rgb(255, 220, 0)"     -- ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á Working
else
    colorCode = "rgb(255, 70, 70)"     -- ‡πÅ‡∏î‡∏á Noob Executor
end

ResultLabel:Set(
    '<font color="' .. colorCode .. '"><b>Unc Test Executer:  ' .. percent .. ' %</b></font>'
)
   
   local status, color
   if percent >= 95 then
      status = "Your Executer UNC Test:  100 %   üëç"
      color = "rgb(0,255,100)"
   elseif percent >= 85 then
      status = "Your Executer Is So Good  ‚úÖ"
      color = "rgb(0,220,255)"
   elseif percent >= 70 then
      status = "Your Executer Is So Normally  ‚úÖ"
      color = "rgb(255,215,0)"
   else
      status = "Executer Your Noobs Solara / Xeno   ‚Üí  üî¥  ‚Üê"
      color = "rgb(255,60,60)"
   end
   
   StatusLabel:Set('<font color="'..color..'"><b>' .. status .. '</b></font>')

   -- ‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡∏´‡∏•‡∏±‡∏á 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
   task.delay(5, function()
      ResultLabel:Set("  ---  ")  -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ Label ‡∏ß‡πà‡∏≤‡∏á (‡∏´‡∏≤‡∏¢‡πÑ‡∏õ)
      StatusLabel:Set("  ---  ")
   end)
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏° Execute
Tab:CreateButton({
   Name = "  Execute UNC Test  ",
   Callback = function()
      if isTesting then return end  -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏î‡∏ã‡πâ‡∏≥
      
      isTesting = true
      
      -- ‡πÄ‡∏£‡∏¥‡πà‡∏° animation Loading . . .
      local dotIndex = 1
      local connection
      
      connection = game:GetService("RunService").Heartbeat:Connect(function()
         if not isTesting then
            connection:Disconnect()
            return
         end
         
         StatusLabel:Set("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏î‡∏™‡∏≠‡∏ö UNC" .. dots[dotIndex])
         dotIndex = (dotIndex % #dots) + 1
      end)
      
      -- ‡∏£‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö (‡πÉ‡∏ä‡πâ task.spawn ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ UI ‡∏Ñ‡πâ‡∏≤‡∏á)
      task.spawn(function()
         local percent = performUNCTest()
         
         -- ‡∏´‡∏¢‡∏∏‡∏î animation
         isTesting = false
         if connection then connection:Disconnect() end
         
         -- ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏•‡∏±‡∏û‡∏ò‡πå
         showResult(percent)
      end)
   end,
})

Tab:CreateSection("")

-- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏£‡∏¥‡∏°
Tab:CreateParagraph({
   Title = "Additional Information",
   Content = "‚Ä¢ The Higher The percentage, the more scripts it supports.\n‚Ä¢ Testing can be repeated.\n‚Ä¢ Good executors in 2026 typically achieve 92-100%."
})

Tab:CreateLabel("Press Execute and Watch The Progress Bar Show Us Who Got 100%   üëç")

Tab:CreateSection("")
