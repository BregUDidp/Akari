local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "   |   Akari Hub   |    Executer Script    |" ,
   Icon = "webhook",
   LoadingTitle = "   Akari Hub  Executer   ",
   LoadingSubtitle = "",
   ShowText = " üåë ",
   Theme = "Default",
   ToggleUIKeybind = "G",
   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Akari"
   },
   KeySystem = false
})

local ExecuteTab = Window:CreateTab("||   - Executer Script", "play") -- Title, Image

ExecuteTab:CreateSection("")
ExecuteTab:CreateSection("Paste Your Script Here")
ExecuteTab:CreateSection("")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏µ‡πà‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏û‡∏¥‡∏°‡∏û‡πå
local CurrentScript = ""

local Input = ExecuteTab:CreateInput({
   Name = "Code Script Anything Input",
   PlaceholderText = "‚úèÔ∏è  100 % UNC Executed",
   RemoveTextAfterFocusLost = false,  -- ‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÑ‡∏ß‡πâ‡∏´‡∏•‡∏±‡∏á‡∏≠‡∏≠‡∏Å‡πÇ‡∏ü‡∏Å‡∏±‡∏™
   Callback = function(Text)
      CurrentScript = Text  -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ñ‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô
   end
})

ExecuteTab:CreateSection("")

ExecuteTab:CreateButton({
   Name = "                                                                      ‚ñ∂Ô∏è",
   Callback = function()
      local scriptCode = CurrentScript
      
      if scriptCode == "" or scriptCode:match("^%s*$") then
         Rayfield:Notify({
            Title = "Error Execute Script ‚ö†Ô∏è",
            Content = "Please enter the code before pressing Execute!",
            Duration = 1,
            Image = "play",
         })
         return
      end
      
      
      
      -- ‡∏£‡∏±‡∏ô‡πÇ‡∏Ñ‡πâ‡∏î‡πÅ‡∏ö‡∏ö‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
      task.spawn(function()
         local success, errorMsg = pcall(function()
            loadstring(scriptCode)()
         end)
         
         if success then
            
         else
            
         end
      end)
   end,
})

-- ‡∏õ‡∏∏‡πà‡∏° Clear (‡πÉ‡∏´‡∏°‡πà!)
ExecuteTab:CreateButton({
   Name = "                                                                      ‚ùå",
   Callback = function()
      CurrentScript = ""           -- ‡∏•‡πâ‡∏≤‡∏á‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£
      Input:Set("")                 -- ‡∏•‡πâ‡∏≤‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡∏ä‡πà‡∏≠‡∏á Input (Rayfield ‡∏°‡∏µ method Set ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Input)
      
      Rayfield:Notify({
         Title = "Cleared Script",
         Content = "The script channel has been successfully cleared.",
         Duration = 1,
         Image = "brush-cleaning",
      })
   end,
})

ExecuteTab:CreateSection("")

-- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏£‡∏¥‡∏°
ExecuteTab:CreateParagraph({
   Title = "Advice",
   Content = "‚Ä¢ Paste your Lua code here.\n‚Ä¢ Press CLEAR to clear the field before starting again.\n‚Ä¢ If you encounter an error, please refer to the message above."
})

ExecuteTab:CreateLabel("Press EXECUTE and let me know if the script worked! üöÄ")

ExecuteTab:CreateSection("")
